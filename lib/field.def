def field
    where
        F: set,
        +: F prod F -> F,
        *: F prod F -> F
    forall a, b, c in F,
        (a + b) + c = a + (b + c),
        a + b = b + a,
        exists 0 in F st forall a in F then a + 0 = 0 + a = a,
        exists -a in F st -a + a = -a + a = 0,
        (a * b) * c = a * (b * c),
        a * b = b * a,
        exists 1 in F st forall a in F then 1 * a = a * 1 = 1,
        forall a in F \ {0} then exists a^(-1) st a * a^(-1) = a^(-1) * a = 1,
        a * (b + c) = a * b + a * c